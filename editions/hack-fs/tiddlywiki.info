{
    "description": "Hacking edition",
    "plugins": [
	"tiddlywiki/filesystem",
        "tiddlywiki/highlight",
	"ustuehler/caption",
	"ustuehler/mdc-web-cdn",
	"ustuehler/mdc",
	"ustuehler/muuri",
	"ustuehler/core",
	"ustuehler/github"
    ],
    "themes": [
	"ustuehler/material"
    ],
    "includeWikis": {
	"path": "../public"
    },
    "build": {
        "index": [
	    "--output",".",
	    "--rendertiddler","$:/core/save/all","hack.html","text/plain"]
    },
    "config": {
	"retain-original-tiddler-path": true
    }
}
